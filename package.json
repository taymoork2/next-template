{
  "name": "next-template",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "preview": "next build && next start",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "next lint",
    "lint:js:fix": "next lint --fix",
    "lint:styles": "stylelint --allow-empty-input **/*.{scss,css}",
    "lint:styles:fix": "stylelint --allow-empty-input --fix **/*.{scss,css}",
    "prettier:write": "prettier --write \"**/*.{js,ts,tsx,mdx,json}\" --cache",
    "prettier:check": "prettier --check \"**/*.{js,ts,tsx,mdx,json}\" --cache",
    "ts:typecheck": "tsc --noEmit --pretty",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "vitest": "vitest --ui",
    "prepare": "husky install"
  },
  "lint-staged": {
    "//": "/* moved to .lintstagedrc.cjs */"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "@t3-oss/env-nextjs": "^0.4.1",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "lucide-react": "0.246.0",
    "next": "^13.4.6",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.32.1",
    "tailwind-merge": "^1.13.2",
    "tailwindcss-animate": "^1.0.6",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@playwright/test": "^1.35.1",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitest/ui": "^0.32.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.43.0",
    "eslint-config-next": "13.4.6",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "playwright": "^1.35.1",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "stylelint": "^15.8.0",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  }
}
